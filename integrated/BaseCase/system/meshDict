FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "rocket_box.stl";

maxCellSize 5.0;

boundaryCellSize 0.1;

minCellSize 3;



localRefinement
{
    "modelrocket.*"
    {
        cellSize 0.0005;
    } 
}

boundaryLayers
{
  //nLayers 15;

  //thicknessRatio 1.2;

  //maxFirstLayerThickness 0.5;

    patchBoundaryLayers
    {
        "modelrocket.*"
        {
            nLayers           10;

            thicknessRatio    1.2;

            maxFirstLayerThickness 0.1;

            allowDiscontinuity 0;
        }


    }
}

renameBoundary
{
    defaultName		fixedWalls;
    defaultType		wall;

    newPatchNames
    {
        "modelrocket.*"
        {
            newName	rocket;
	    type     wall;
        }
 
        "zMax"
        {
            newName     inlet;
            type     patch;
        }

        "zMin"
        {
            newName     outlet;
            type     patch;
        }
    }
}
               
     
